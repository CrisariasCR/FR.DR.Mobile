DEFINE FORMATO_CORTO "XXXXXXXXXXXXXXX|LEFT"
DEFINE FORMATO_LARGO "XXXXXXXXXXXXXXXXXXXX|LEFT"
DEFINE FORMATO_NUMERO "¢#######0.00|RIGHT"
DEFINE FORMATO_FECHA "dd/MM/yyyy"
DEFINE FORMATO_LINEA_DEVOLUCION "XX|LEFT" "XXXXXX|LEFT" "XXXXXXXXXXXX|LEFT" "#0.00|RIGHT" "#0.00|RIGHT" "¢###0.00|RIGHT"

PRINT "\n\n\r"
FONT "IS204" 10
								   
FOREACH DEVOLUCION IN DEVOLUCIONES.LISTA_DEVOLUCIONES
	PRINT "Devolución\n\r"
	PRINT "Número:   " DEVOLUCION.NUMERO "\n\r"
	PRINT "NCF: " DEVOLUCION.NCF "\n\n\r"
	PRINT "Fecha:    " 
	PRINTF FORMATO_FECHA DEVOLUCION.FECHA "\n\r"
	PRINT "Cliente:  " 
	PRINTF FORMATO_CORTO DEVOLUCIONES.CLIENTE_NOMBRE  "\n\r"
	PRINT "Dirección:" 
	PRINTF FORMATO_LARGO DEVOLUCIONES.CLIENTE_DIRECCION "\n\r"
	PRINT "Compañia: " DEVOLUCION.COMPANIA "\n\n\r"
	
	PRINT "Detalle Devolución" "\n\r"
	PRINT "                      Unidades \n\r"
	PRINT "  Cód   Descripción   Alm  Det   Monto\n\r"	
							
	FOREACH LINEA_DEVOLUCION IN DEVOLUCION.DETALLE_DEVOLUCION	
		PRINTF FORMATO_LINEA_DEVOLUCION LINEA_DEVOLUCION.ESTADO LINEA_DEVOLUCION.CODIGO LINEA_DEVOLUCION.DESCRIPCION LINEA_DEVOLUCION.ALMACEN LINEA_DEVOLUCION.DETALLE LINEA_DEVOLUCION.MONTO "\n\r"		
	NEXT	
	
	PRINT "\n\r"
	PRINT "SubTotal:       "
	PRINTF FORMATO_NUMERO DEVOLUCION.SUB_TOTAL "\n\r"
	
	PRINT "Imp.Venta:      "
	PRINTF FORMATO_NUMERO DEVOLUCION.IMP_VENTA "\n\r"

	PRINT "Imp.Consumo:    "
	PRINTF FORMATO_NUMERO DEVOLUCION.IMP_CONSUMO "\n\r"
	
	PRINT "Descuento Lin:  "
	PRINTF FORMATO_NUMERO DEVOLUCION.DESCUENTO "\n\r"
	
	PRINT "Descuento 1:    "
	PRINTF FORMATO_NUMERO DEVOLUCION.DESCUENTO_UNO "\n\r"
	
	PRINT "Descuento 2:    "
	PRINTF FORMATO_NUMERO DEVOLUCION.DESCUENTO_DOS "\n\r"

	PRINT "Total:          "
	PRINTF FORMATO_NUMERO DEVOLUCION.TOTAL "\n\n\r"
	
	PRINT "B: Buen estado.\n\r"
	PRINT "M: Mal estado.\n\n\r"
	
	PRINTF FORMATO_LARGO DEVOLUCION.LEYENDA "\n\n\r"	
NEXT	

PRINT "\n\n\r"
					
							
