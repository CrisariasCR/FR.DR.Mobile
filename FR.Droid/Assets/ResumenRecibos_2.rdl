DEFINE FORMATO_CORTO "XXXXXXXXXXXXXXX|LEFT"
DEFINE FORMATO_LARGO "XXXXXXXXXXXXXXXXXXXX|LEFT"
DEFINE FORMATO_NUMERO "$#######0.00|RIGHT"
DEFINE FORMATO_FECHA "dd/MM/yyyy"

DEFINE LINEA_RECIBO "XXXXXXXXX|LEFT" "XXXXXXXXXX|LEFT" "dd/MM/yyyy|RIGHT" "$#######.00|RIGHT"
DEFINE LINEA_MONTO "XXXXXXXXX|LEFT" "$#######.00|RIGHT"

PRINT "\n\n\r"
FONT "IS204" 10

PRINT "RESUMEN DE RECIBOS\n\n\r"
PRINT "Cliente:"
PRINTF FORMATO_CORTO RECIBOS.CLIENTE_NOMBRE "\n\r"
PRINT "Compañia    Recibo     Fecha" "\n\r"

FOREACH RECIBO IN RECIBOS.LISTA_RECIBOS
	PRINTF LINEA_RECIBO RECIBO.COMPANIA RECIBO.NUMERO RECIBO.FECHA "\n"
	PRINTF LINEA_MONTO  "Monto:  " RECIBO.MONTO "\n\r"
NEXT

PRINT "\n\n\r"