DEFINE FORMATO_CORTO "XXXXXXXXXXXXXXX|LEFT"
DEFINE FORMATO_LARGO "XXXXXXXXXXXXXXXXXXXX|LEFT"
DEFINE FORMATO_NUMERO "XXXXXXXXXXXXXXXXXX|LEFT" "$#######0.00|RIGHT"
DEFINE FORMATO_FECHA "dd/MM/yyyy"

PRINT "\n\n\r"
FONT "IS204" 10

PRINT "Reporte de Devoluciones\n\r"
PRINT "Fecha:"
PRINTF FORMATO_FECHA TRANSACCIONES.FECHA "\n\r"

PRINT "\n\n\r"

FOREACH CLIENTE IN TRANSACCIONES.CLIENTES

	PRINT "Código:" CLIENTE.CODIGO "\n\r"
	PRINT "Cliente:" 
	PRINTF FORMATO_CORTO CLIENTE.NOMBRE "\n\r"
	PRINTF FORMATO_NUMERO "Dev.Sin Documento:" CLIENTE.DEVUELTOSINDOC "\n\r"
	PRINTF FORMATO_NUMERO "Dev.Con Documento:" CLIENTE.DEVUELTOCONDOC "\n\r"	
	PRINTF FORMATO_NUMERO "Total Devuelto:"  CLIENTE.DEVUELTOTOTAL "\n\n\r"	

NEXT

PRINTF FORMATO_NUMERO "Total Devuelto:" TRANSACCIONES.TOTAL_DEVUELTO

PRINT "\n\n\n\r"