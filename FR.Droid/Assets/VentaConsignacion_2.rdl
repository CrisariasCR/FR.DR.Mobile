DEFINE FORMATO_LARGO "XXXXXXXXXXXXXXXXXXXX|LEFT"
DEFINE FORMATO_NUMERO "$#######0.00|RIGHT"
DEFINE FORMATO_FECHA "dd/MM/yyyy"
DEFINE FORMATO_LINEA_CONSIGNACION "XXXXX|LEFT"  "#0.00|RIGHT" "#0.00|RIGHT" "$#####0.00|RIGHT"
DEFINE FORMATO_LINEA_CONSIGNACION_HEADER "XXXXX|LEFT"  "XXXXX|RIGHT" "XXXXX|RIGHT" "XXXXXXXXXX|RIGHT"

PRINT "\n\n\r"
FONT "IS204" 10

FOREACH VENTACONSIGNACION IN VENTASCONSIGNACION.LISTA_CONSIGNACIONES

	PRINT "Venta en Consignación\n\r"
	PRINT "Fecha:     " 
	PRINTF FORMATO_FECHA VENTACONSIGNACION.FECHA 
	PRINT "\n\r"
	PRINT "Cliente:   " 
	PRINTF FORMATO_LARGO VENTASCONSIGNACION.CLIENTE_NOMBRE "\n\r"
	PRINT "Compañia:  " VENTACONSIGNACION.COMPANIA "\n\r"
	PRINT "Condición: " VENTACONSIGNACION.CONDICION "\n\n\r"

	PRINT "Detalle de la venta en consignación\n\r"
	PRINT "         Unidades          \n\r"
	PRINTF FORMATO_LINEA_CONSIGNACION_HEADER "Cód Alm Det Monto "\n\r"	

	FOREACH LINEA_CONSIGNACION IN VENTACONSIGNACION.DETALLES
		PRINTF FORMATO_LINEA_CONSIGNACION LINEA_CONSIGNACION.CODIGO LINEA_CONSIGNACION.ALMACEN LINEA_CONSIGNACION.DETALLE LINEA_CONSIGNACION.MONTO "\n\r"
	NEXT

	PRINT "\n\r"
	PRINT "SubTotal:     "
	PRINTF FORMATO_NUMERO VENTACONSIGNACION.SUB_TOTAL "\n\r"

	PRINT "Imp.Venta:    " 
	PRINTF FORMATO_NUMERO VENTACONSIGNACION.IMP_VENTA "\n\r"

	PRINT "Imp.Consumo:  "  
	PRINTF FORMATO_NUMERO VENTACONSIGNACION.IMP_CONSUMO "\n\r"

	PRINT "Descuento 1:  " 
	PRINTF FORMATO_NUMERO VENTACONSIGNACION.DESCUENTO_UNO "\n\r"

	PRINT "Descuento 2:  " 
	PRINTF FORMATO_NUMERO VENTACONSIGNACION.DESCUENTO_DOS "\n\r"

	PRINT "Total:        " 
	PRINTF FORMATO_NUMERO VENTACONSIGNACION.TOTAL "\n\n\r"
	
	PRINTF FORMATO_LARGO VENTACONSIGNACION.LEYENDA "\n\n\r"
NEXT
