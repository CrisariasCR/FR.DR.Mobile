DEFINE FORMATO_CORTO "XXXXXXXXXXXXXXX|LEFT"
DEFINE FORMATO_LARGO "XXXXXXXXXXXXXXXXXXXX|LEFT"
DEFINE FORMATO_NUMERO "¢#######0.00|RIGHT"
DEFINE FORMATO_FECHA "dd/MM/yyyy"

PRINT "\n\n\r"
FONT "IS204" 10

PRINT "Reporte de Visitas\n\r"
PRINT "Fecha:" 
PRINTF FORMATO_FECHA VISITAS.FECHA "\n\r"

FOREACH VISITA IN VISITAS.LISTA_VISITAS
	
	PRINT "Código: " VISITA.CODIGO_CLIENTE "\n\r"
	
	PRINT "Cliente:" VISITA.NOMBRE_CLIENTE "\n\r"
	
	PRINT "Estado:" VISITA.DESCRIPCION "\n\r"
	
	PRINT "Hora:" VISITA.HORA "\n- - - - - - - - -\n\r"

NEXT

PRINT "\n\n\r"