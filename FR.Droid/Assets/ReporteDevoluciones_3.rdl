DEFINE FORMATO_CORTO "XXXXXXXXXXXXXXX|LEFT"
DEFINE FORMATO_LARGO "XXXXXXXXXXXXXXXXXXXXXXXXXX|LEFT"
DEFINE FORMATO_NUMERO "$#######0.00|RIGHT"
DEFINE FORMATO_FECHA "dd/MM/yyyy"

PRINT "\n\n\r"
FONT "IS204" 10

PRINT "Reporte de Devoluciones\n\r"
PRINT "Fecha:"
PRINTF FORMATO_FECHA TRANSACCIONES.FECHA "\n\r"

PRINT "\n\n\r"

FOREACH CLIENTE IN TRANSACCIONES.CLIENTES

	PRINT "Código:" CLIENTE.CODIGO "\n\r"
	PRINT "Cliente:" 
	PRINTF FORMATO_CORTO CLIENTE.NOMBRE "\n\r"
	PRINT "Monto Devuelto Sin Documento:         " 
	PRINTF FORMATO_NUMERO CLIENTE.DEVUELTOSINDOC "\n\r"
	PRINT "Monto Devuelto Con Documento:		 "
	PRINTF FORMATO_NUMERO CLIENTE.DEVUELTOCONDOC "\n\r"
	PRINT "Monto Total Devuelto:         " 
	PRINTF FORMATO_NUMERO CLIENTE.DEVUELTOTOTAL "\n\n\r"	

NEXT

PRINT "Total Devuelto:"
PRINTF FORMATO_NUMERO TRANSACCIONES.TOTAL_DEVUELTO

PRINT "\n\n\n\r"